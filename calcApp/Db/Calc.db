# -----------------------  Booster ----------------------
record(calc, "$(P):BO_PR_1"){
    field(CALC, "(A+B+C+D+E+F+G+H+I+J+K+L)/12.")
    field(EGU, "mBar")

    field(INPA, "BO-01U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPB, "BO-04U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPC, "BO-05D:VA-CCG-RFC:Pressure-Mon MSS CP")
    field(INPD, "BO-06U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPE, "BO-09U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPF, "BO-11U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPG, "BO-14U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPH, "BO-16U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPI, "BO-19U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPJ, "BO-21U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPK, "BO-24U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPL, "BO-26U:VA-CCG-ED:Pressure-Mon MSS CP")
}

record(calc, "$(P):BO_PR_2"){
    field(CALC, "(A+B+C+D+E+F+G+H+I+J)/10.")
    field(EGU, "mBar")

    field(INPA, "BO-29U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPB, "BO-31U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPC, "BO-34U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPD, "BO-36U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPE, "BO-39U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPF, "BO-41U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPG, "BO-44U:VA-CCG-BG:Pressure-Mon MSS CP")
    field(INPH, "BO-46U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPI, "BO-47U:VA-CCG-ED:Pressure-Mon MSS CP")
    field(INPJ, "BO-49U:VA-CCG-BG:Pressure-Mon MSS CP")
}
    
record(calc, "$(P):VA-CCG-BO-Avg:Pressure-Mon"){
    field(CALC, "(A+B)/2.")
    field(EGU, "mBar")
    field(DESC, "CCG Average Pressure per Sector")

    field(INPA, "$(P):BO_PR_1 CP MSS")
    field(INPB, "$(P):BO_PR_2 CP MSS")
}

